lines := split(readfile("input.txt"), "\n")

total := 0

for _, battery in lines {
    digits := list()
    len := len(battery)
    for i in range(len) {
        push(digits, int(get(battery, i)))
    }

    max := -1
    for i in range(len) {
        for j in range(i+1, len) {
            value := get(digits, i) * 10 + get(digits, j)
            if value > max {
                max = value
            }
        }
    }

    total = total + max
}

print("Answer:", total)
